<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-MissingRefreshTokenReqParams">
    <DisplayName>AM-MissingRefreshTokenReqParams</DisplayName>
    <FaultRules/>
    <Properties/>
    <AssignVariable>
        <Name>api-error.error</Name>
        <Value>missing_request_params</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.code</Name>
        <Value>NB_40002</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.message</Name>
        <Value>FormParams grant_type, refresh_token is mandatory in request.</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.status_code</Name>
        <Value>400</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.reason_phrase</Name>
        <Value>Bad Request</Value>
        <Ref/>
    </AssignVariable>
    <!-- This variable is used to raise fault as it will not be resolved -->
    <AssignVariable>
        <Name>api-error.raise_fault</Name>
        <Ref>api-error.variable_raise_fault</Ref>
    </AssignVariable>
    <!-- Set to false to raise fault -->
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"/>
</AssignMessage>
