<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-PingResponsePayload">
    <DisplayName>AM-PingResponsePayload</DisplayName>
    <Set>
        <Payload variablePrefix="@" variableSuffix="#" contentType="application/json">{ "message" : "Pong", "responseTime" : "@responseTime#"}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"></AssignTo>
</AssignMessage>
