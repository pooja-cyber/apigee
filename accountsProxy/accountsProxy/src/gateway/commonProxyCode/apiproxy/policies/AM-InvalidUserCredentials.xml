<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-InvalidUserCredentials">
    <DisplayName>AM-InvalidUserCredentials</DisplayName>
    <FaultRules/>
    <Properties/>
    <AssignVariable>
        <Name>api-error.error</Name>
        <Value>invalid_user_credentails</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.code</Name>
        <Value>NB_40100</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.message</Name>
        <Value>Invalid User Credentails, Please check username and password.</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.status_code</Name>
        <Value>401</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.reason_phrase</Name>
        <Value>Unauthorized</Value>
        <Ref/>
    </AssignVariable>
    <!-- This variable is used to raise fault as it will not be resolved -->
    <AssignVariable>
        <Name>api-error.raise_fault</Name>
        <Ref>api-error.variable_raise_fault</Ref>
    </AssignVariable>
    <!-- Set to false to raise fault -->
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"/>
</AssignMessage>
