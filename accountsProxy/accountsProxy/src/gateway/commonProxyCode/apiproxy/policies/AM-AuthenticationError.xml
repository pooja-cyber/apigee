<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-AuthenticationError">
    <DisplayName>AM-AuthenticationError</DisplayName>
    <FaultRules/>
    <Properties/>
    <AssignVariable>
        <Name>api-error.error</Name>
        <Value>Authentication Error</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.code</Name>
        <Value>NB_40100</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.message</Name>
        <Value>Authentication Error from Target System</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.status_code</Name>
        <Value>401</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.reason_phrase</Name>
        <Value>Unauthorized</Value>
        <Ref/>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"/>
</AssignMessage>
