<HTTPTargetConnection>
    <LoadBalancer>
        <MaxFailures>0</MaxFailures>
        <RetryEnabled>true</RetryEnabled>
        <Server name="TOBEREPLACED">
            <IsEnabled>true</IsEnabled>
        </Server>
    </LoadBalancer>
    <Path>{du.config.oracleCmsTargetBasepath}</Path>
    <Properties>
        <Property name="success.codes">1xx,2xx,3xx,4xx</Property>
    </Properties>
</HTTPTargetConnection>