<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="KVM-GetTargetConfigurations" mapIdentifier="portability-v1-config">
    <DisplayName>KVM-GetTargetConfigurations</DisplayName>
    <Properties/>
    <Get assignTo="du.config.mceTargetBasepath">
        <Key> 
            <Parameter>mceTargetBasepath</Parameter>
        </Key>
    </Get>
    <Get assignTo="du.config.mceUsername">
        <Key>
            <Parameter>mceUsername</Parameter>
        </Key>
    </Get>
    <Get assignTo="du.config.mcePassword">
        <Key>
            <Parameter>mcePassword</Parameter>
        </Key>
    </Get>
    <Get assignTo="du.config.applicationId">
        <Key>
            <Parameter>mceApplicationId</Parameter>
        </Key>
    </Get>
    <Get assignTo="du.config.spikeArrest">
        <Key>
            <Parameter>spikeArrest</Parameter>
        </Key>
    </Get>
    <!-- Portability API Flags -->
    <Get assignTo="du.config.isCancelPortabilityRequestFlag">
        <Key>
            <Parameter>isCancelPortabilityRequestFlag</Parameter>
        </Key>
    </Get>
    <Get assignTo="du.config.isGetSchedulingFlag">
        <Key>
            <Parameter>isGetSchedulingFlag</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>